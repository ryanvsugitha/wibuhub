generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model credential {
  username String @id @db.VarChar(50)
  password String @db.VarChar(50)
}

model favorites {
  username String @id @db.VarChar(50)
  mal_id   String @db.VarChar(50)
}

model user_detail {
  username   String @id @db.VarChar(50)
  user_email String @db.VarChar(50)
}
